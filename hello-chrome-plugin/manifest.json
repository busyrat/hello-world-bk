{
	"manifest_version": 2,
	"name": "VIP直通车",
	"version": "0.1",
	"description": "免VIP了，啦啦啦啦",
	"author": "busyrat",
	"icons": {
		"48": "icon.png",
		"128": "icon.png"
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": [
				"https://www.baidu.com/*"
			],
			"js": [
				"content-script.js"
			],
			"css": ["content.css"]
		}
	],
	"web_accessible_resources": [
		"inject.js"
	],
	"permissions": [
		"tabs"
	]
}